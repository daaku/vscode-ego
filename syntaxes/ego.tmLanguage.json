{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Ego",
	"patterns": [
		{
			"include": "#go"
		},
		{
			"include": "#html"
		}
	],
	"repository": {
		"html": {
			"patterns": [
				{
					"include": "text.html.basic"
				}
			]
		},
		"go": {
			"begin": "<%",
			"end": "%>",
			"patterns": [
				{
					"include": "source.go"
				}
			]
		}
	},
	"scopeName": "source.ego"
}
